    <div class="row" id="mainList">
        <div class="col">
            <div class="bd-example">
                <div class="card">
                    <div class="card-header ">
                        <div class="row text-center">
                            <div class="col-sm font-weight-bold">Name</div>
                            <div class="col-sm font-weight-bold">Script</div>
                            <div class="col-sm font-weight-bold">VO</div>
                            <div class="col-sm font-weight-bold">Research</div>
                            <div class="col-sm font-weight-bold">Editor</div>
                            <div class="col-sm font-weight-bold">Master</div>
                            <div class="col-sm font-weight-bold">Captions</div>
                        </div>
                    </div>
                    <div class="card-body" style="overflow: auto">
                        <%for (var result = 0; result < 100; result++){%>
                        <div id="<%=pods[result].pod_id%>" class="row border" order="<%=pods[result].pod_name%>">
                            <div class="col-sm text-center my-auto" style="text-overflow: ellipsis; overflow: hidden; ">
                                <button class="btn btn-link font-weight-bold" onclick="openModal('<%=pods[result].pod_id%>')">
                                    <%=pods[result].pod_name%> </button>
                            </div>
                            <% if (pods[result].writer == null && pods[result].writer == "") { %>
                            <div class="col-sm text-center my-auto"><button onclick="addScriptDiv('<%=pods[result].pod_id%>')"
                                    class="btn btn-danger btn-sm" data-toggle="tooltip" data-placement="left" title='<%=pods[result].writer%>'>
                                    <i class="fas fa-times"></i> Missing </button></div>
                            <% } else { %>
                            <div class="col-sm text-center my-auto"><button onclick="addScriptDiv('<%=pods[result].pod_id%>')"
                                    class="btn btn-success btn-sm" data-toggle="tooltip" data-placement="left" title='<%=pods[result].writer%>'>
                                    <i class="fas fa-check"></i> Complete
                                </button></div>
                            <% } %>

                            <% if ((pods[result].voTalent == null || pods[result].voTalent == "")) { %>
                            <div class="col-sm text-center my-auto"><button onclick="addVOtalentDiv('<%=pods[result].pod_id%>')"
                                    class="btn btn-danger btn-sm" data-toggle="tooltip" data-placement="left" title='<%=pods[result].voTalent%>'>
                                    <i class="fas fa-times"></i> Missing </button></div>
                            <% } else { %>
                            <div class="col-sm text-center my-auto"><button onclick="addVOtalentDiv('<%=pods[result].pod_id%>')"
                                    class="btn btn-success btn-sm" data-toggle="tooltip" data-placement="left" title='<%=pods[result].voTalent%>'>
                                    <i class="fas fa-check"></i> Complete</button></div>
                            <% } %>

                            <% if ((pods[result].researcher == null || pods[result].researcher == "")) { %>
                            <div class="col-sm text-center my-auto"><button onclick="addResearchDiv('<%=pods[result].pod_id%>')"
                                    class="btn btn-danger btn-sm" data-toggle="tooltip" data-placement="left" title='<%=pods[result].researcher%>'>
                                    <i class="fas fa-times"></i> Missing </button></div>
                            <% } else { %>
                            <div class="col-sm text-center my-auto"><button onclick="addResearchDiv('<%=pods[result].pod_id%>')"
                                    class="btn btn-success btn-sm" data-toggle="tooltip" data-placement="left" title='<%=pods[result].researcher%>'>
                                    <i class="fas fa-check"></i> Complete
                                </button></div>
                            <% } %>

                            <% if ((pods[result].editor == null || pods[result].editor == "")) { %>
                            <div class="col-sm text-center my-auto"><button onclick="addEditDiv('<%=pods[result].pod_id%>')"
                                    class="btn btn-danger btn-sm" data-toggle="tooltip" data-placement="left" title='<%=pods[result].editor%>'>
                                    <i class="fas fa-times"></i> Missing </button></div>
                            <% } else { %>
