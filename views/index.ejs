<script>
    var podList = <%-JSON.stringify(pods)%>

    document.getElementById("displayedCount").innerHTML = "Displaying Records: " + podList.length + " of " +
        podList.length

    $(document).ready(function () {
        $('.modal-content .fa-check').toggleClass('d-none');
        $('i').click(function () {
            if ($(this).parents().siblings('input').is('[readonly]')) {
                $(this).parents().siblings('input').prop('readonly', false); //turns the readonly off
                $(this).toggleClass('d-none'); //d-none one glyphicon
                $(this).siblings('.fa-pencil-alt').toggleClass('d-none');
            } else {
                $(this).toggleClass('d-none'); //d-none one glyphicon
                $(this).siblings('.fa-check').toggleClass('d-none');
                $(this).parents().siblings('input').prop('readonly', true); //turns the readonly off
            }
        });
    });

    function addVOtalentDiv(id) {
        addVOtalentDiv = document.getElementById('addVOtalentDiv')
        addVOtalentValue = document.getElementById('addVOtalent')
        if (podList[id - 1].voTalent != null || podList[id - 1].voTalent != undefined) {
            addVOtalentValue.value = podList[id - 1].voTalent
        }
        addVOtalentDiv.style.display = "block"
    }

    function addResearchDiv(id) {
        addResearch = document.getElementById('addResearchDiv')
        addResearchValue = document.getElementById('addResearch')
        if (podList[id - 1].researcher != null || podList[id - 1].researcher != undefined) {
            addResearchValue.value = podList[id - 1].researcher
        }
        addResearch.style.display = "block"
    }

    function addScriptDiv(id) {
        podScript = document.getElementById('addScriptDiv')
        podScriptValue = document.getElementById('addScript')
        if (podList[id - 1].writer != null || podList[id - 1].writer != undefined) {
            podScriptValue.value = podList[id - 1].writer
        }
        podScript.style.display = "block"
    }

    function addEditDiv(id) {
        addEdit = document.getElementById('addEditDiv')
        addEditValue = document.getElementById('addEdit')
        if (podList[id - 1].editor != null || podList[id - 1].editor != undefined) {
            addEditValue.value = podList[id - 1].editor
        }
        addEdit.style.display = "block"
    }

    function addMasterDiv(id) {
        addMaster = document.getElementById('addMasterDiv')
        addMasterValue = document.getElementById('addMaster')
