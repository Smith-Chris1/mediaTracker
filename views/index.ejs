    function closeModal(modal) {
        document.getElementById(modal).style.display = "none"
    }

    function close_all() {
        for (i = 0; i < podList.length; i++) {
            document.getElementById(podList[i].pod_id).classList.add('d-none')
        }
    }

    function show_all() {
        for (i = 0; i < podList.length; i++) {
            document.getElementById(podList[i].pod_id).classList.remove('d-none')
        }
    }

    function find_my_div() {
        close_all();
        displayingCount = 0
        var str_needle = edit_search.value;
        str_needle = str_needle.toUpperCase();
        var searchStrings = str_needle.split(/\W/);

        for (var i = 0, len = searchStrings.length; i < len; i++) {
            var currentSearch = searchStrings[i].toUpperCase();
            if (currentSearch !== "") {
                for (var j = 0; j < podList.length; j++) {
                    if (podList[j].pod_name.toUpperCase().indexOf(currentSearch) !== -1) {
                        document.getElementById(podList[j].pod_id).classList.remove('d-none')
                        displayingCount++
                    }
                }
                document.getElementById("displayedCount").innerHTML = "Displaying Records: " +
                    displayingCount + " of " + podList.length
            } else {
                show_all();
                document.getElementById("displayedCount").innerHTML = "Displaying Records: " + podList.length +
                    " of " + podList.length
            }
        }
    }
</script>

